howPrivate: false,\n      showShared: false,\n    });\n  };\n  const showShared = () => {\n    props.setOpen({\n      ...props.open,\n      showFolders: false,\n      showPrivate: false,\n      showShared: true,\n    });\n  };\n\n  const showPrivate = () => {\n    props.setOpen({\n      ...props.open,\n      showFolders: false,\n      showPrivate: true,\n      showShared: false,\n    });\n  };\n\n  const selectFile = (event) => {\n    props.setOpen({\n      ...props.open,\n      fileId: event.target.value,\n    });\n  };\n\n  const openPrivate = () => {\n    let file = props.open.private.find(\n      (x) => parseInt(x.id) === parseInt(props.open.fileId)\n    );\n    console.log(file);\n    props.setCode({\n      ...props.code,\n      name: file.name,\n      extension: file.extension,\n      content: file.content,\n    });\n    props.setState({\n      ...props.state,\n      showMenu: false,\n      showCalendar: false,\n      showCodePopUp: false,\n    });\n  };\n\n  const openShared = () => {\n    let file = props.open.shared.find(\n      (x) => parseInt(x.id) === parseInt(props.open.fileId)\n    );\n    props.setCode({\n      ...props.code,\n      name: file.name,\n      extension: file.extension,\n      content: file.content,\n    });\n    props.setState({\n      ...props.state,\n      showMenu: false,\n      showCalendar: false,\n      showCodePopUp: false,\n    });\n  };\n  const mapPrivate = props.open.private.map((file) => {\n    return (\n      <File\n        key={file.id}\n        id={file.id}\n        name={file.name}\n        extension={file.extension}\n        selectFile={selectFile}\n      />\n    );\n  });\n\n  const mapShared = props.open.shared.map((file) => {\n    return (\n      <File\n        key={file.id}\n        id={file.id}\n        name={file.name}\n        extension={file.extension}\n        selectFile={selectFile}\n      />\n    );\n  });\n\n  return (\n    <>\n      {props.open.showFolders ? (\n        <div className=\"code-open\">\n          <div className=\"open-file-system\" onClick={showPrivate}>\n            <img\n              alt=\"logo\"\n              id=\"open-private-icon\"\n              className=\"open-file-icon\"\n              src=\"/img/icons/open.png\"\n            />\n            <h2 className=\"open-file-h3\"> Private</h2>\n          </div>\n          <div className=\"open-file-system\" onClick={showShared}>\n            <img\n              alt=\"logo\"\n              id=\"open-shared-icon\"\n              className=\"open-file-icon\"\n              src=\"/img/icons/open.png\"\n            />\n            <h2 className=\"open-file-h3\"> Shared</h2>\n          </div>\n        </div>\n      ) : null}\n      {props.open.showShared ? (\n        <div className=\"open-file-shared\">\n          <select\n            className=\"file-list\"\n            name=\"file-list\"\n            size={props.open.shared.length + 1}\n          >\n            {mapShared}\n          </select>\n          <div className=\"open-file-buttons\">\n            <button onClick={showFolders} className=\"open-code-button\">\n              Back\n            </button>\n            <button onClick={openShared} className=\"open-code-button\">\n              Open\n            </button>\n          </div>\n        </div>\n      ) : null}\n      {props.open.showPrivate ? (\n        <div className=\"open-file-private\">\n          <select\n            className=\"file-list\"\n            name=\"file-list\"\n            size={props.open.private.length + 1}\n          >\n            {mapPrivate}\n          </select>{' '}\n          <div className=\"open-file-buttons\">\n            <button onClick={showFolders} className=\"open-code-button\">\n              Back\n            </button>\n            <button onClick={openPrivate} className=\"open-code-button\">\n              Open\n            </button>\n          </div>\n        </div>\n      ) : null}\n    </>\n  );\n}\n","/home/marcel/lighthouse/login/src/components/Code/File.